<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.pocketcampus.plugin.food.android"
      android:versionCode="1"
      android:versionName="1.0">
      
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application android:icon="@drawable/icon" android:label="@string/app_name"
        android:theme="@style/PocketCampusTheme"
		android:name="org.pocketcampus.android.platform.sdk.core.GlobalContext">
        
        <activity android:name="org.pocketcampus.plugin.food.android.FoodMainView"
                  android:label="Food">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
       	<activity android:name="org.pocketcampus.plugin.food.android.FoodSuggestionsView"></activity>

		<service android:name="org.pocketcampus.plugin.food.android.FoodController"
			xmlns:android="http://schemas.android.com/apk/res/android"></service>

    </application>
</manifest>