<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  android:id="@+id/food_sandwich_lign_list"
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"  android:layout_height="wrap_content"
  android:orientation="horizontal">
  
	<TextView 
  		android:id="@+id/food_sandwich_place"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textColor="@android:color/white" android:padding="6dip" 
		android:paddingLeft="15dip"/>
	
	<LinearLayout
		android:id="@+id/rela2"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:orientation="vertical" android:layout_gravity="right"
		android:paddingLeft="15dip" android:paddingRight="25dip">
				
		<CheckBox 
			android:id="@+id/food_sandwich_left_checkbox" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_gravity="right" android:clickable="false"
			android:paddingLeft="15dip"/>
	
	</LinearLayout>
		
</LinearLayout>
