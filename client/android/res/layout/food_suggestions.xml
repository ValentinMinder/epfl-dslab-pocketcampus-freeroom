<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<org.pocketcampus.core.ui.ActionBar
		android:id="@+id/actionbar" style="@style/ActionBar" />

	<RelativeLayout
		android:id="@+id/food_suggestions_rela"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@+id/actionbar">
		
		<Button android:id="@+id/food_suggestions_button" 
		android:layout_width="wrap_content" android:layout_height="wrap_content"/>
			
		<ListView android:id="@+id/food_suggestions_list" 
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:drawSelectorOnTop="false" android:fastScrollEnabled="true"
			android:cacheColorHint="#00000000" android:layout_below="@+id/food_suggestions_button" />	
			
		
</RelativeLayout>
	
</RelativeLayout>
