<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout android:layout_width="wrap_content"
	xmlns:android="http://schemas.android.com/apk/res/android"
	style="@style/Theme.MainLayout"
	android:padding="15dip" 
	android:layout_height="wrap_content"
	>
	
	<TextView android:textStyle="bold"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/supervisors_title"
		android:text="@string/mainscreen_supervisors"></TextView>
	
	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/supervisors"
		android:layout_below="@+id/supervisors_title"
		android:text="@string/mainscreen_supervisors_names"></TextView>
	
	<TextView android:textStyle="bold"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:id="@+id/developers_title"
		android:layout_below="@+id/supervisors"
		android:text="@string/mainscreen_developers"></TextView>

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@+id/developers_title"
		android:id="@+id/developers"
		android:text="@string/mainscreen_developers_names"></TextView>

</RelativeLayout>
